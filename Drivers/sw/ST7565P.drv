%-************************************************************************************************************
%-INTERNAL_METHOD_BEG WriteCmd
%define! Parcmd
%include Common\GeneralInternalGlobal.inc (WriteCmd)
static void WriteCmd(uint8_t cmd)
{
  CLR_A0();                                                      %>40/* A0 low */
  CLR_CS();                                                      %>40/* CS1 low: select device */
  (void)%@SPI@'ModuleName'%.SendChar(cmd);                       %>40/* send command byte */
  SET_CS();                                                      %>40/* CS high: deselect device */
}

%-INTERNAL_METHOD_END WriteCmd
%-************************************************************************************************************
%-INTERNAL_METHOD_BEG WriteData
%define! Pardata
%include Common\GeneralInternalGlobal.inc (WriteData)
static void WriteData(uint8_t data)
{
  SET_A0();                                                      %>40/* A0 low */
  CLR_CS();                                                      %>40/* CS1 low: select device */
  (void)%@SPI@'ModuleName'%.SendChar(data);                      %>40/* send data byte */
  SET_CS();                                                      %>40/* CS high: deselect device */
}

%-INTERNAL_METHOD_END WriteData
%-************************************************************************************************************
%-BW_METHOD_BEGIN Clear
%ifdef Clear
%include Common\ST7565PClear.Inc
void %'ModuleName'%.%Clear(void)
{
  short ddram;

  for (ddram=%'ModuleName'%.DISPLAY_HW_NOF_COLUMNS*(%'ModuleName'%.DISPLAY_HW_NOF_ROWS/8); ddram>0; ddram--) {
    WriteData(0);                                                %>40 /* clear actual line */
  }
}

%endif %- Clear
%-BW_METHOD_END Clear
%-************************************************************************************************************
%-BW_METHOD_BEGIN Init
%ifdef Init
%include Common\ST7565PInit.Inc
void %'ModuleName'%.%Init(void)
{
  /* boot display */
  SET_RES();                                                     %>40 /* RES high: start reset sequence */
  SET_CS ();                                                     %>40 /* CS high: Disable device */
  %@Wait@'ModuleName'%.Waitms(10);                               %>40 /* wait at least 10ms */
  CLR_RES();                                                     %>40 /* Reset line low */
  %@Wait@'ModuleName'%.Waitms(100);                              %>40 /* wait at least 100ms */
  SET_RES();                                                     %>40 /* Awake LCD from RESET state. */
  %@Wait@'ModuleName'%.Waitms(10);                               %>40 /* wait at least 10ms */

  /* initialize display */
  %'ModuleName'%.%Clear();                                       %>40 /* Erase all pixel on the DDRAM */
  WriteCmd(0xA0);                                                %>40 /* ADC segment driver direction normal */
  WriteCmd(0xA2);                                                %>40 /* added 1/9 bias */
  WriteCmd(0xA4);                                                %>40 /* Display all points: normal */
  WriteCmd(0xA6);                                                %>40 /* Sets LCD display normal */

  WriteCmd(0xC0);                                                %>40 /* COM output scan direction normal */
  WriteCmd(0x2F);                                                %>40 /* Select internal power supply operating mode */

  WriteCmd(0x40);                                                %>40 /* Display start line set to 0 */

  WriteCmd(0x25);                                                %>40 /* set the resistor ratio */
  WriteCmd(0x81);                                                %>40 /* electronic volume mode set */
  WriteCmd(0x02);                                                %>40 /* electronic volume register set */

  WriteCmd(0xAF);                                                %>40 /* LCD display on */
}

%endif %- Init
%-BW_METHOD_END Init
