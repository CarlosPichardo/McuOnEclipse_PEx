%-BW_METHOD_BEGIN UpdateFull
%ifdef UpdateFull
%include Common\ST7565PUpdateFull.Inc
void %'ModuleName'%.%UpdateFull(void)
{
  uint8_t page;
  uint8_t column;

  /* iterate throught the pages */
  for(page = 0; page < ((%'ModuleName'%.DISPLAY_HW_NOF_ROWS-1)/8)+1; page++) {
    WriteCmd(0xB0 | page);                                       %>40 /* Sets actual display RAM page */
    WriteCmd(0x10);                                              %>40 /* Sets actual display RAM column address to 0 */
    WriteCmd(0x00);
    /* iterate through the columns */
    for(column = 0; column < %'ModuleName'%.DISPLAY_HW_NOF_COLUMNS; column++) {
      WriteData(NH1_DisplayBuf[page][NH1_DISPLAY_HW_NOF_COLUMNS-1-column]);
    }
  }
}

%endif %- UpdateFull
%-BW_METHOD_END UpdateFull
