%-BW_METHOD_BEGIN SetMode
%ifdef SetMode
%define! Parinverted
%include Common\ST7565PSetMode.Inc
void %'ModuleName'%.%SetMode(bool inverted)
{
  if (inverted) {
    WriteCmd(0xA6|1);                                            %>40 /* Inverted display mode */
  } else {
    WriteCmd(0xA6);                                              %>40 /* Normal display mode */
  }
}

%endif %- SetMode
