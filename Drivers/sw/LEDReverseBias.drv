%-BW_METHOD_BEGIN StopSafeReverseBias
%ifdef StopSafeReverseBias
%define! ParPortBackup
%define! ParportBackup
%include Common\LEDReverseBiasStopSafeReverseBias.Inc
void %'ModuleName'%.%StopSafeReverseBias(void * portBackup)
{
  /* revert back to previous state */
  %@CathodePin@'ModuleName'%.SetOutput();
  %@AnodePin@'ModuleName'%.PutVal(((%'ModuleName'%.PortStatus *)portBackup)->anode);
  %@CathodePin@'ModuleName'%.PutVal(((%'ModuleName'%.PortStatus *)portBackup)->cathode);
}

%endif %- StopSafeReverseBias
