%-BW_METHOD_BEGIN StartSafeReverseBias
%ifdef StartSafeReverseBias
%define! ParPortBackup
%define! ParportBackup
%include Common\LEDReverseBiasStartSafeReverseBias.Inc
void %'ModuleName'%.%StartSafeReverseBias(void *portBackup)
{
  /* make backup of current port status */
  ((%'ModuleName'%.PortStatus *)portBackup)->anode = %@AnodePin@'ModuleName'%.GetVal();
  ((%'ModuleName'%.PortStatus *)portBackup)->cathode = %@CathodePin@'ModuleName'%.GetVal();
  /* reverse bias */
  %@AnodePin@'ModuleName'%.ClrVal();
  %@CathodePin@'ModuleName'%.SetVal();
  /* start discharge phase */
  %@CathodePin@'ModuleName'%.SetInput();
}

%endif %- StartSafeReverseBias
%-BW_METHOD_END StartSafeReverseBias
