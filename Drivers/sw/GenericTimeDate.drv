%else
    res = RTC2_GetDate(Date);
%endif
    if (res!=ERR_OK) {
      return res;
    }
  }
  return ERR_OK;
#else
  (void)Time;
  (void)Date;
  return ERR_FAILED; /* no external RTC available */
#endif
}

