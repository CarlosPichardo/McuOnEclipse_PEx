%-BW_METHOD_BEGIN GetInternalRTCTimeDate
%ifdef GetInternalRTCTimeDate
%define! ParTime
%define! ParDate
%define! RetVal
%include Common\GenericTimeDateGetInternalRTCTimeDate.Inc
uint8_t %'ModuleName'%.%GetInternalRTCTimeDate(TIMEREC *Time, DATEREC *Date)
{
#if %'ModuleName'%.USE_INTERNAL_HW_RTC_LDD
  LDD_RTC_TTime timeDate;

%if defined(LDDRTC)
  %@LDDRTC@'ModuleName'%.GetTime(%@LDDRTC@'ModuleName'%.DeviceData, &timeDate); /* get information from HW RTC */
%else
  RTC1_GetTime(RTC1_DeviceData, &timeDate); /* get information from HW RTC */
%endif
  if (Time!=NULL) {
    Time->Hour = timeDate.Hour;
    Time->Min = timeDate.Minute;
    Time->Sec = timeDate.Second;
#if %'ModuleName'%.HAS_SEC100_IN_TIMEREC
    Time->Sec100 = 0;
#endif
  }
  if (Date!=NULL) {
    Date->Year = timeDate.Year;
    Date->Month = timeDate.Month;
    Date->Day = timeDate.Day;
  }
#elif %'ModuleName'%.USE_INTERNAL_HW_RTC_BEAN
  TIMEREC time;
  DATEREC date;
  uint8_t res;

  if (Time!=NULL) {
  %if defined(RTC)
    res = %@RTC@'ModuleName'%.GetTime(&time); /* get information from HW RTC */
  %else
    res = RTC1_GetTime(&time); /* get information from HW RTC */
  %endif
    if (res!=ERR_OK) {
      return res;
    }
    *Time = time; /* struct copy */
  }
  if (Date!=NULL) {
  %if defined(RTC)
    res = %@RTC@'ModuleName'%.GetDate(&date); /* get information from HW RTC */
  %else
    res = RTC1_GetDate(&date); /* get information from HW RTC */
  %endif
    if (res!=ERR_OK) {
      return res;
    }
    *Date = date; /* struct copy */
  }
#else
  /* no HW RTC configured, return default time */
  *Time = %'ModuleName'%.DefaultTime; /* struct copy */
#endif
  return ERR_OK;
}

