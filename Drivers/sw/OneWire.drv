  PROG pr;

  if(%@Program@'ModuleName'%.NofFreeElements()==0) {
    return ERR_FAILED;
  }
  pr.Instr = I_RESET;
  (void)%@Program@'ModuleName'%.Put(*(uint8_t*)&pr);
  if(!Data.Busy) { /* start action */
    %'ModuleName'%.i_action();
