%-*****************************************************************************************************
%-BW_METHOD_BEGIN add_CRC
%ifdef add_CRC
%define! ParbitValue
%include Common\OneWireadd_CRC.inc
void %'ModuleName'%.%add_CRC(uint8_t bitValue)
{
  bool fb;

  fb = (Data.CRC & 0x01) ^ bitValue;
  Data.CRC >>= 1;
  if(fb) {
    Data.CRC ^= 0x8C;
  }
}
%endif %- add_CRC
%-BW_METHOD_END add_CRC

