    Data.Step = TS_NOTHING;
    Data.Error = OWERR_NO_DEVICE;
  %ifdef OnError
    %OnError(Data.Error);
  %endif
  } else {
    Data.Error = OWERR_OK;
    %'ModuleName'%.i_action();
  }
}
%endif %-i_presence
